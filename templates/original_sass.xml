<templateSet group="original_sass">
  <template name="inc" value="@include" description="sassインクルード" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="ext" value="@extend" description="sassエクステンド" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="for" value="@for $i from $befoeIndex$ through $afterIndex$ {&#10;    $roperty${&#10;        $value$&#10;    }&#10;}" description="sassのfor文 変数は #{$i} と入れる" toReformat="false" toShortenFQNames="true">
    <variable name="befoeIndex" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="afterIndex" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="roperty" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="value" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mq-pc" value="@include mq-pc(){&#10;    $END$&#10;}" description="mq-pc mixin" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mq-sp" value="@include mq-sp(){&#10;    $END$&#10;}" description="mq-sp mixin" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="fzSp" value="@include fzSp($END$);" description="fzSp mixin" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="fzPc" value="@include fzPc($END$);" description="fzPc mixin" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="position" value="$el$.getBoundingClientRect();//エレメントの絶対位置&#10;let windowPositionTop = document.body.getBoundingClientRect().top;//windowの相対位置" description="エレメントの絶対位置とwindowの相対位置" toReformat="false" toShortenFQNames="true">
    <variable name="el" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>